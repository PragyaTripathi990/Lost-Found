{
  "name": "lost-found-app",
  "version": "1.0.0",
  "description": "AI-powered Lost & Found application with semantic search",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && npm start",
    "setup": "./scripts/setup.sh",
    "dev": "./scripts/start-dev.sh",
    "install-all": "cd frontend && npm install && cd ../backend && npm install && cd ../python-ai && pip install -r requirements.txt",
    "start-backend": "cd backend && npm run dev",
    "start-ai": "cd python-ai && source venv/bin/activate && python app.py",
    "start-frontend": "cd frontend && npm run dev",
    "test": "echo 'No tests specified' && exit 0"
  },
  "keywords": [
    "lost",
    "found",
    "ai",
    "search",
    "clip",
    "embeddings",
    "react",
    "nodejs",
    "postgresql",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/lost-found-app.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.8.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "@supabase/supabase-js": "^2.38.4",
    "pg": "^8.11.3",
    "node-fetch": "^3.3.2"
  }
}
